<p class="promotion-subtitle"><span style="font-size: 10px; color: rgb(255, 255, 255);">Last leaderboard update: <span id="update-time" style="font-size: 10px; color: rgb(255, 255, 255);"></span></span></p>
<script>
  function formatTime(date) {
    const pad = (n) => n.toString().padStart(2, '0');
    return `${pad(date.getUTCDate())}/${pad(date.getUTCMonth() + 1)}/${date.getUTCFullYear()} - ${pad(date.getUTCHours())}:00 GMT`;
  }

  function setLastUpdate() {
    const now = new Date();
    const gmtHours = now.getUTCHours();
    let updateHour;

    if (gmtHours < 12) {
      now.setUTCDate(now.getUTCDate() - 1);
      updateHour = 19;
    } else if (gmtHours < 19) {
      updateHour = 12;
    } else {
      updateHour = 19;
    }

    const updateDate = new Date(Date.UTC(
      now.getUTCFullYear(),
      now.getUTCMonth(),
      now.getUTCDate(),
      updateHour,
      0
    ));

    document.getElementById("update-time").textContent = formatTime(updateDate);
  }

  setLastUpdate();
</script>
